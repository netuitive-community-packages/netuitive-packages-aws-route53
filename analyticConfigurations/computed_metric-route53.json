{
  "analyticConfiguration": {
    "metrics": [
      {
        "match": "aws.route53.healthcheckstatus.all",
        "properties": {
          "expression": "data.sum('aws.route53.healthcheckstatus.*') * 100 / data.count('aws.route53.healthcheckstatus.*')",
          "fqn": "aws.route53.healthcheckstatus.all",
          "name": "Indicating the % of healthy regions."
        }
      }
    ],
    "name": "Route53",
    "scope": {
      "elementType": "Route53HealthCheck"
    },
    "type": "COMPUTED_METRIC"
  }
}